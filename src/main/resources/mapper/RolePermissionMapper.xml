<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linovce.blog.mapper.RolePermissionMapper">
    <resultMap id="BaseResultMap" type="com.linovce.blog.entity.RolePermission">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="id" jdbcType="BIGINT" property="id" />
        <result column="pid" jdbcType="BIGINT" property="pid" />
        <result column="rid" jdbcType="BIGINT" property="rid" />
    </resultMap>

    <select id="selectAll" resultType="com.linovce.blog.entity.RolePermission">
            select * from role_permission
    </select>

    <select id="selectById" parameterType="int" resultType="com.linovce.blog.entity.RolePermission">
            select * from   role_permission  where id= #{id}
        </select>
</mapper>

